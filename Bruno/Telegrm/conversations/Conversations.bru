meta {
  name: Conversations
  type: http
  seq: 1
}

get {
  url: http://localhost:8080/api/conversations
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

script:post-response {
  var id = res.body[0].id
  bru.setEnvVar("conversationID", id)
}

settings {
  encodeUrl: true
  timeout: 0
}
